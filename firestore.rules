rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if request.auth.token.email.matches("firebase@flutterflow.io");
    }

    match /user/{parent}/Category_Attributes/{document} {
      allow create: if false;
      allow read: if false;
      allow write: if false;
      allow delete: if false;
    }

    match /{path=**}/Category_Attributes/{document} {
      allow read: if false;
    }


    match /user/{parent}/Historic_Data/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth.uid == parent;
      allow delete: if false;
    }

    match /{path=**}/Historic_Data/{document} {
      allow read: if request.auth != null;
    }


    match /Tournament/{parent}/II_statistics/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /{path=**}/II_statistics/{document} {
      allow read: if request.auth != null;
    }


    match /Match/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /{path=**}/Match_events/{document} {
      allow read: if request.auth != null;
    }


    match /Tournament/{parent}/TTournament_Stats/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /{path=**}/TTournament_Stats/{document} {
      allow read: if request.auth != null;
    }


    match /user/{parent}/UI_Statistics/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth.uid == parent;
      allow delete: if false;
    }

    match /{path=**}/UI_Statistics/{document} {
      allow read: if request.auth != null;
    }


    match /user/{parent}/UV_Statistics/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth.uid == parent;
      allow delete: if false;
    }

    match /{path=**}/UV_Statistics/{document} {
      allow read: if request.auth != null;
    }


    match /chat_messages/{document} {
      allow create: if /databases/$(database)/documents/user/$(request.auth.uid) in get(request.resource.data.chat).data.users
&& /databases/$(database)/documents/user/$(request.auth.uid) == request.resource.data.user;
      allow read: if /databases/$(database)/documents/user/$(request.auth.uid) in get(resource.data.chat).data.users;
      allow write: if false;
      allow delete: if false;
    }

    match /chats/{document} {
      allow create: if /databases/$(database)/documents/user/$(request.auth.uid) in request.resource.data.users;
      allow read: if /databases/$(database)/documents/user/$(request.auth.uid) in resource.data.users;
      allow write: if /databases/$(database)/documents/user/$(request.auth.uid) in resource.data.users;
      allow delete: if false;
    }

    match /user/{document} {
      allow create: if request.auth != null;
      allow read: if true;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /{path=**}/Category_Attributes/{document} {
      allow read: if false;
    }


    match /{path=**}/Historic_Data/{document} {
      allow read: if request.auth != null;
    }


    match /Tournament/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /{path=**}/II_statistics/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/Match_events/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/TTournament_Stats/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/UI_Statistics/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/UV_Statistics/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/Category_Attributes/{document} {
      allow read: if false;
    }


    match /teams/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /Field/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if request.auth != null;
      allow delete: if false;
    }

    match /{path=**}/Match_events/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/TTournament_Stats/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/II_statistics/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/UI_Statistics/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/UV_Statistics/{document} {
      allow read: if request.auth != null;
    }


    match /{path=**}/Historic_Data/{document} {
      allow read: if request.auth != null;
    }


    match /Category_info/{document} {
      allow create: if request.auth != null;
      allow read: if request.auth != null;
      allow write: if false;
      allow delete: if false;
    }
  }
}